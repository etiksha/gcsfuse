[global]
ioengine=libaio
direct=1
fadvise_hint=0
verify=0
bs=1M
iodepth=64
invalidate=1
ramp_time=10s
runtime=60s
time_based=1
nrfiles=1
thread=1 
directory=gcs/3mb
openfiles=1
group_reporting=1
allrandrepeat=1
